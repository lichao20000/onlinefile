@CHARSET "UTF-8";
.editbtn {display:inline-block; width:16px; height:16px; background:url(../../public/img/edit.jpg) no-repeat no-repeat; cursor:pointer;}
#reserve table td{padding-top:10px;}

.find-dialog{ width:203px; height:20px; border:1px solid #aaa; background:#fff; float: left; margin-top: 2px; line-height: 20px;position: absolute;right: 2px;}
.find-dialog input{ width:180px; height:20px; line-height:20px; float:left; border:0; _line-height:23x; color: gray;}
.find-dialog span{ width:20px; height:20px; float:left;  background:url(../img/search.gif) no-repeat center center; cursor:pointer;}

.reserveSearch{width:203px; height:20px; border:1px solid #aaa; background:#fff; float: right; margin-top: 2px;margin-bottom: 8px; line-height: 20px; }
.reserveSearch input{ width:180px; height:20px; line-height:20px; float:left; border:0; _line-height:23px; color: gray;}
.reserveSearch span{ width:20px; height:20px; float:left;  background:url(../img/search.gif) no-repeat center center; cursor:pointer;}

.flexigrid div.tDiv0 /*toolbar*/{background:#ffffff url(images/bg.gif) repeat-x top;position:relative;padding-top:5px;border-bottom:0;overflow:hidden}
.flexigrid div.tDiv1{float:left;clear:both;padding:1px}

html,body{ margin:0; padding:0; overflow:hidden;}

.leftDiv{ width:180px; float:left; overflow:hidden;}
.rightDiv{ float:left; overflow:hidden;}

.leftDiv div{ width:100%; height:auto; overflow:hidden; float:left;}
.leftDiv a, .leftDiv h2{ width:175px; height:30px; font:normal normal normal 14px/30px 微软雅黑,黑体,宋体,arial; text-indent:20px; float:left;}
.leftDiv h2{ color:#3E78FD; font-weight:bold;}
.leftDiv a{ border-left:5px solid #fff; font-size:12px; text-decoration:none; color:#000;}
.leftDiv a:hover{ border-left:5px solid #f5f5f5; background:#f5f5f5;}
.leftDiv a.selected{ border-left:5px solid #F24343; background:#eee;}
.leftDiv span{ color:#000;}
.opens{ width:16px; height:16px; display:inline-block; background:url(../img/editor.gif) no-repeat no-repeat; cursor:pointer;}

.ESAppClientReserveContainer{
	width:96%;
	margin-left:2%;
}
