@CHARSET "UTF-8";
body{ margin:0 auto; padding:0; overflow:hidden;}
html{ overflow:hidden;}
input,p,ul,li{ margin:0; padding:0;}
ul{list-style-type:none;}


/* 动态添加select下拉框样式
 * filetypePos 		用做定位和设定宽高
 * filetypeSeledbox 被选中文种类型和文种列表DIV
 * Seledfiletype	被选中文种类型DIV
 * filetypelist		文种类型列表
 *
 */
 .Seledfiletype{float:left;padding:1px;margin-left:16px;}
 .provinceselect{float:left;padding:1px;}
 .selectype{margin-top:2px;}
 .province{margin-top:3px;}

 #messageInfo{float:left;font-size:12px;color:red;padding:1px;margin-top:3px;margin-left:16px;}

/* 筛选功能通用样式 */
.h2{ width:100%;  line-height:25px; text-indent:25px; color:#555; border-bottom:1px solid #f3f3f3; float:left; margin:5px 0; background:#f9f9f9 url(../../public/img/mess.png) no-repeat 5px 5px;}
#addrolefree{ width:655px; height:auto; overflow:hidden; float:left;}
#contents{ width:650px; height:150px; overflow:auto; marign-left:5px; _dispaly:inline;}
.contbox{ width:645px; height:30px; float:left; margin-left:5px; _display:inline;}
.w14090{ font-size:13px; color:#555; float:left; margin-right:10px; display:block;}
.hei30{ line-height:30px;}
.w140{ width:130px; padding-left:2px;}
.w90{ width:80px;}
.hei20{ height:20px; line-height:20px; margin-top:4px; border:1px solid #aaa;}

/* 添加行删除行按钮样式 */
.newfilter,.delfilter{ width:70px; line-height:24px; text-indent:24px; color:#555; float:left; margin-top:3px;}
.newfilter{ margin-right:10px; background:url(../../public/img/add.png) no-repeat 4px 4px;}
.delfilter{ background:url(../../public/img/del.gif) no-repeat 4px 4px;}
.newfilter:hover,.delfilter:hover{ width:68px; line-height:22px; text-indent:23px; border:1px solid #e3e3e3;}
.newfilter:hover{ background:#f3f3f3 url(../../public/img/add.png) no-repeat 3px 3px;}
.delfilter:hover{ background:#f3f3f3 url(../../public/img/del.gif) no-repeat 3px 3px;}
/* 筛选功能通用样式结束 */
.esshowtree{height:220px;width:162px;background:#fff;border:1px solid #aaa;display:none;z-index:888;position:absolute;overflow:auto;}

.extra{display:none;width:70px;}
.extra .extraretention{width:70px;}