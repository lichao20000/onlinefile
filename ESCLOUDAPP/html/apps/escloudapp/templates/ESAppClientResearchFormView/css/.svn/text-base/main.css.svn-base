@charset "utf-8";
/* CSS Document */

.leftDiv{ width:180px; float:left; overflow:hidden;}
.rightDiv{ float:left; overflow:hidden;}

.leftDiv div{ width:100%; height:auto; overflow:hidden; float:left;}
.leftDiv a, .leftDiv h2{ width:175px; height:30px; font:normal normal normal 14px/30px 微软雅黑,黑体,宋体,arial; text-indent:20px; float:left;}
.leftDiv h2{ color:#3E78FD; font-weight:bold;}
.leftDiv a{ border-left:5px solid #fff; font-size:12px; text-decoration:none; color:#000;}
.leftDiv a:hover{ border-left:5px solid #f5f5f5; background:#f5f5f5;}
.leftDiv a.selected{ border-left:5px solid #F24343; background:#eee;}
.leftDiv span{ color:#000;}
.opens{ width:16px; height:16px; display:inline-block; background:url(../img/editor.gif) no-repeat no-repeat; cursor:pointer;}

body,div,p,h3,img,span,strong,dl,dt,dd,ul,li{margin:0;padding:0;border:0;list-style:none;}body{ background-color:#eeeeee;font:62.5%/1.5 微软雅黑,tahoma,arial,宋体,sans-serif; color:#404040;list-style:none;width:100%;margin:0 auto;}.wrap{ width:100%;}.main{ width:88%; margin:3% 3% 1% 3%; padding:3%; background-color:#FFF; border:1px solid #e2e2e2; position:relative;}.main img {width:27%;max-width:100%;max-height:100%;}.main h3{ width:94%; height:1.5em; line-height:1.5em; color:#bd0000;  font-weight:normal;font-size:1.2em;}.main p{ width:94%; line-height:1.7em; color:#000; font-size:1.2em; margin-bottom:4%;}.main p.current{ margin-bottom:0px;}.main p span{color:#2492dd;}.main p strong{color:#e7562e;font-weight:normal;}.dow{width:96%;margin:0 auto;padding:2%;background:#eeeeee;}.download {color:black;}.logo {float:left;width:10%;display:inline;margin:1.5% 0 0 1.5%;}.logo img {max-width:100%;max-height:100%;}.logo_nei {float:left;width:58%;margin-left:2%;display:inline;}.logo_nei_con {color:#353535;font-size:1.5em;margin-top:6%;}.button {float:right;width:25%;display:inline;margin-top:1%;margin-right:1%;}.button img {max-width:100%;}.clear {height:0;overflow:hidden;clear:both;}br{line-height:0px;}

topicTitle {TEXT-ALIGN: center;}
#center { MARGIN-RIGHT: auto; MARGIN-LEFT: auto; }