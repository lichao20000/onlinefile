.borrowSelect{ width:100%; height:auto;overflow:hidden; float:left;}
.borrowSelect p{ width:100%; height:30px; float:left; cursor:default;}
.borrowSelect p input{ float:left; margin:8px 0 0 15px; _margin-top:5px;}
.borrowSelect p span{ float:left; margin:8px 0 0 10px;}
