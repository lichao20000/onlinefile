@CHARSET "UTF-8";

body{ margin:0 auto; padding:0;} /**/
body,html{ overflow:hidden;}
input{ margin:0; padding:0;}
select{_font-size:12px;}
ul{ margin:0; padding:0; list-style-type:none;}
h2,p,form,table{ margin:0; padding:0; font-weight:normal;}
/*档案著录筛选文本样式*/
.inputOperate{ float:left; height:26px; width:210px;}
.formForStatus{height: 25px;width:215px;border-bottom: 1px solid #ccc;margin-top:10px;color:#fff;}
.usingTreeDemo{width:100%;  float:left; overflow:auto; padding:0px;width:220px;}
.fill{background:url(../../public/flexigrid/css/images/zoom_in.png) no-repeat 2px center;}
.searchIn{height:18px; line-height:18px; width:140px; float:left; margin:3px 3px 0 0;}
.inputOperate div{ float:left; _width:auto; margin-top:2px; border:1px solid transparent; _border-color:tomato; _filter:chroma(color=tomato); cursor:pointer;}
.inputOperate span{ float:left; height:20px; line-height:20px; padding:0 5px 0 20px; border-top:1px solid transparent; border-left:1px solid transparent; _border-top-color:tomato; _border-left-color:tomato; _filter:chroma(color=tomato);}
.search{cursor:pointer;}
.inputOperate .extbtncssdiv{ border:1px solid #ccc;}
.inputOperate .extbtncsspan{ border-top:1px solid #fff; border-left:1px solid #fff;}
/* form style start*/
.clear:after{content:',';height:0px;visibility:hidden;display:block;clear:both;}
.contentDiv{width:580px;height:220px;}
.leftTv{width:80px;height:23px;line-height:23px;}
.rightTv{width:118px;height:23px;margin-left:10px;text-align:left;line-height:23px;}
.contentDiv ul,li{list-style:none;}
.contentDiv ul{float:left;margin-top:7px;}
.contentDiv ul li{float:left;}
.contentDiv ul li input{width:180px;height:19px;line-height:19px;}
#selectSty{_height:25px;_font-size:13px;}

