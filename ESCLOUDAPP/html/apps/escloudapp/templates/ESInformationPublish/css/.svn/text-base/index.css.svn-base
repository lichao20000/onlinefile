.editortitlebox{ width:100%; height:40px; float:left;}
.summarybox{ width:100%; height:90px; float:left;}
.typebutton{ width:100%; height:30px; float:left; margin:10px 0 20px;}
.onlystyle{ float:left; width:100%; height:auto; overflow:hidden;}
.s{ height:10px; overflow:hidden;}

.fnleftbox{ width:400px; height:30px; float:left;}
