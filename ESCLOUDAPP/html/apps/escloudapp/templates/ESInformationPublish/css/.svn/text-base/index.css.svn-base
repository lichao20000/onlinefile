html,body{ margin:0; padding:0; overflow:hidden;}
input,textarea,h2{ margin:0; padding:0; outline:0; font-size:12px; font-weight:normal;}

.leftfree,.rightfree{ float:left;}

.rightfree{background-color:#fff;}

.leftfree{ overflow:auto;margin-right:10px;background-color:#133959;border-radius:5px;}
.archivelist{ float:left;}
.auto_scroll{ width:100%; float:left; overflow-y:scroll}

.ztree li span.button.add {margin-left:2px; margin-right: -1px; background-position:-144px 0; vertical-align:top; *vertical-align:middle}

