<script type="text/javascript">
var rightStr="<?php echo $str;?>";
var scanPath="<?php echo $scanPath;?>";
var batchModifyStruId="<?php echo $batchModifyStruId;?>";//liqiubo 20141010 这存的是真正的结构ID，如果当前是从案卷级挂接的，则此为卷内级结构，修复bug 1304，主要在存规则的位置使用了
var isupload=0;
var tt=$("#matchingRule").is(':checked');
if(tt){
	$(".exeButton").hide();
}
$("#matchingRule").die().live('click',function(){
	isupload=0;
	$(".exeButton").hide();
});
$("#involveSubFile").die().live('click',function(){
	isupload=1;
	$(".exeButton").show();
});
var trimext = /\|\w+/;
$('#contentL li').die().live('click',function(){
	$('#contentL li').removeClass('esselected');
	$(this).addClass('esselected');
});
//左移
$("#extraleft").die().live('click',function(){
	var st = $('#rightList .esselected').text().match(trimext).toString().substr(1);
	if(st=="FIELD"){
		var temp = $('#rightList .esselected').text().replace(trimext,"");
		var html = $('#rightList .esselected').html(temp);
		$('#leftList').append(html);
	}else{
		$('#rightList .esselected').remove();
	}
});
//右移
$("#extraright").die().live('click',function(){
	var html = $('#leftList .esselected').html();
	html+='|FIELD';
	$('#leftList .esselected').html(html);
	$('#leftList .esselected').appendTo('#rightList');
});
$("#chan").die().live('click',function(){
	var length = $("input[name='comlen']").val();
	if(length==""){
		$.dialog.notice({icon:'error',content:'请先填写连接符',time:3,title:'3秒后自动关闭'});
		return;
	}
	length+='|CONNECTOR';
	var html='<li id="">'+length+'</li>';
	$('#rightList').append(html);
});
//置顶
$("#extratop").die().live('click',function(){
	if($("#rightList .esselected").html()==$('#rightList li:eq(0)').html()){
		return;
	}
	$('#rightList li:eq(0)').before($("#rightList .esselected"));
});
//置底
$("#extrabottom").die().live('click',function(){
	if($("#rightList .esselected").html()==$('#rightList li:last').html()){
		return;
	}
	$('#rightList li:last').after($("#rightList .esselected"));
});
//上移
$("#extraup").die().live('click',function(){
	var index=$('#rightList .esselected').index()-1;
	//guolanrui 20141014 添加index的判断，避免在IE8下选择最上的字段上移时字段消失BUG：1352
	if(index>-1){
		$('#rightList li:eq('+index+')').before($("#rightList .esselected"));
	}
});
//下移
$("#extradown").die().live('click',function(){
	var index=$('#rightList .esselected').index()+1;
	$('#rightList li:eq('+index+')').after($("#rightList .esselected"));
});
//左双击
$('#leftList li').die().live('dblclick',function (){
	var ext = $(this).html();
	ext+='|FIELD';
	$(this).html(ext);
	$('#rightList').append($(this));
});
//右双击
$("#rightList li").die().live('dblclick',function(){
	var st = $(this).text().match(trimext).toString().substr(1);
	//alert(st);return;
	if(st=="FIELD"){
		var temp = $(this).text().replace(trimext,"");
		$(this).html(temp);
		$('#leftList').append($(this));
	}else{
		$(this).remove();
	}
});
//筛选
function showList(value,position)
{
	if($.trim(value)==''){
		$('#leftList li').show();
		$('#rightList li').show();
		return;
	}
	if(position=='left' && $.trim(value)!='')
	{
		$('#leftList li').hide();
		var liObj=$('#leftList li:contains('+value+')').show();
		$('#leftList').append(liObj);
	}else if(position=='right' && $.trim(value)!='')
	{
		$('#rightList li').hide();
		var liObj=$('#rightList li:contains('+value+')').show();
		$('#rightList').append(liObj);
	}else{
		return;
	}
}
function makeNewSWFUpload(){
	var files = [];
	var upload = new SWFUpload({
		//提交路径
		upload_url: "",
		file_post_name: "file.txt",		
		file_size_limit : "1048576",	// 100MB  longjunhao 20140905 修改为1024MB=1GB
		file_types : "*.*",
		file_types_description : "所有文件",
		file_upload_limit : "0",
		file_queue_limit : "0",

		// 事件处理
		swfupload_loaded_handler : swfuploadLoaded,
		file_dialog_start_handler : fileDialogStart,
		file_queued_handler : fileQueued,
		file_queue_error_handler : fileQueueError,
		file_dialog_complete_handler : fileDialogComplete,
		upload_start_handler : uploadStart,
		upload_progress_handler : uploadProgress,
		upload_error_handler : uploadError,
		upload_success_handler : uploadSuccess,
		upload_complete_handler : uploadComplete,

		// 按钮的处理
		button_image_url : "<?php echo $tplPath?>/public/SWFUpload/img/ButtonUpload72.png",
		button_placeholder_id : "btnUpload",
		button_width: 72,
		button_height: 28,
		
		// Flash文件地址设置
		flash_url : "<?php echo $tplPath?>/public/SWFUpload/js/swfupload.swf",
		
		custom_settings : {
			progressTarget : "fsUploadProgress",
			cancelButtonId : "btnCancel",
			startButtonId : "btnStart",
			// 上传成功的回调函数
			uploadSuccess : function(file, data, remainder){
				//alert(file+'-----'+remainder);return;//[object Object]-----0
				var f = $.parseJSON(data);
				var extName = file.name.substr(file.name.lastIndexOf(".")+1);
				extendFile(f.fileId, f.contentMD5, f.fileSize, f.createTime, file.name, extName, remainder);
			}
		},
		
		// Debug 设置
		debug: false
	});
	$("#btnCancel").click(function(){cancelQueue(upload);});
	$("#btnStart").click(function(){
		$.dialog({
			content:'确定要上传吗?确定后操作不可恢复，请谨慎操作！',
			icon:'warning',
			ok:true,
			okVal:'确定',
			cancel:true,
			cancelVal:'取消',
			ok:function(){
				$.post($.appClient.generateUrl({ESIdentify:'getUploadURL'},'x'),  function(data){
					upload.setUploadURL(data);
					startQueue(upload);
				});
			},
			cache:false
		});
	});
	// 挂接文件
	function extendFile(fileid, md5, filesize, createtime, filename, extName, remainder){
		files.push({ORIGINAL_ID:fileid, ESMD5:md5, ESTITLE:filename, ESTYPE:extName, ESSIZE:filesize, EsCreateTime:createtime});
		if(remainder ===0){
			var datas = "";
			var isScan="";
			var temp="";
			$('#rightList li').each(function(i){
	    		temp += $(this).text()+';';
	    		var tmp = $(this).text().match(trimext).toString().substr(1);
	    		if(tmp=="FIELD"){
	    			var id = $(this).attr("id");
	    			datas+=id+'-';
	    		}
			});
			if(temp==rightStr){
				isScan='true';
			}else{
				isScan='false';
			}
			datas=datas.substr(0,datas.length-1);
			var treeNodeId = $('#treeDemo').attr('treenodeid');
			var tempMassHangingFiles = document.getElementsByName("AllORPart");  
			var allOrPartFiles = '';
			for(var i=0;i<tempMassHangingFiles.length;i++)  {     
				if(tempMassHangingFiles[i].checked)           
				 allOrPartFiles = tempMassHangingFiles[i].value;  
			}
			

			var id='';
			var checkboxObj=$("input[name='path']:checked",$('#flexme'));
			if(checkboxObj.length > 0)
			{
				//遍历选中的数据
				checkboxObj.each(function(i){
					id+=$(this).val()+',';
					})
			}
			var condition='';
			if(!id){
				condition=filterValue();
				var form=$('#esfilter');
					var flag=form.validate();
					if(!flag){
						return false;
					}
			}
			$.post($.appClient.generateUrl({ESIdentify:'massHangingFiles'},'x'),{struId:strucid,busiModelId:status,matchFile:datas,files:files,isScan:isScan,treeNodeId:treeNodeId,allOrPart:allOrPartFiles,pkgPath:id,condition:condition},function(data){
				if(data=='true'){
					files=[];
					$.dialog.list['hangingDialog'].close();
					$.dialog.list['massHangingDialog'].close();
					$.dialog.notice({width: 150,content: '成功挂接数据',icon: 'succeed',time: 3});
					$("#flexme,#innerfile").flexReload();
				}else if(data=='notMatchData'){
					$.dialog.notice({title:'操作提示',icon:'warning',content:'没有匹配的数据！',time:3});
					$.dialog.list['hangingDialog'].close();
					$.dialog.list['massHangingDialog'].close();
					return false;
				}else if(data=='false'){
					$.dialog.notice({width: 150,content: '挂接数据失败',icon: 'error',time: 3});
					return false;
				}
			});
		}
	};
};
</script>