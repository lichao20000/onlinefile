/** xiaoxiong 20140825 添加可编辑下来框组件样式 **/
.editSelect {
	margin-left:80px;
}
.editSelect font{
	font-size:9pt;
}
.editSelect .selectSpan {
	border-top:1pt solid #c1c1c1;
	border-right:1pt solid #c1c1c1;
	border-bottom:1pt solid #c1c1c1;
	overflow:hidden;
	width:17px;
	height:20px;
	float:left;
	clip:rect(-1px 162px 162px 142px);
}
.editSelect .selectSpan select{
	width:162px;
	height:20px;
	border:0pt;
	margin:0px 0px 0px -145px;
}
.editSelect .selectInputSpan{
	border-top:1pt solid #c1c1c1;
	border-left:1pt solid #c1c1c1;
	border-bottom:1pt solid #c1c1c1;
	width:142px;
	height:20px;
	padding:0px;
	border:false;
	padding:0px;
	float:left;
}
.editSelect .selectInputSpan .selectInput{
	width:142px;
	float:left;
	border:0pt;
	font-size:9pt; 
	padding:0px;
	margin:0px;
	background-color: white;
}
