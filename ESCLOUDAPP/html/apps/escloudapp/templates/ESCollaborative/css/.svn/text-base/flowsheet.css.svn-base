/* 档案销毁和信息发布流程共用样式 */
.flowsheet_paper{ width:800px; height:445px; float:left; overflow:hidden;}
.flowsheet_full{ width:800px; height:418px; float:left; overflow-y:scroll; position:relative;}

/* 表单标题、表单号、表单*/
.flowsheet_full .form_title{ width:780px; height:40px; line-height:40px; text-align:center; font-size:26px; float:left;}
.flowsheet_full .form_number{ width:765px; height:25px; line-height:25px; text-align:right; font-size:12px; float:left;}

.flowsheet_full .flowsheet_form{ width:748px; height:auto; float:left; margin-left:15px; border:2px solid #000; _display:inline;}
.flowsheet_form ul,.flowsheet_form .div{ width:748px; height:auto; float:left; border-bottom:1px solid #000; overflow:hidden;}
.flowsheet_form .clear_bottom_border{ border-bottom:0;}
.flowsheet_form li{ height:31px; line-height:30px; font-size:14px; float:left;}

.flowsheet_form .field_name{ width:90px; text-align:center; background:#D9D9D9; border-right:1px solid #000; padding:0 5px;}
.flowsheet_form .short_field_value{ width:262px; padding:0 5px;}
.flowsheet_form .long_field_value{ width:636px; padding:0 5px;}

.border_right{ border-right:1px solid #000;}

.flowsheet_form .reset_height{ height:120px;}/* 控制审批描述,意见标签高度 */
.flowsheet_form .opinions_textarea{ width:635px; height:110px; line-height:18px; font-size:14px; float:left; margin:5px 0; border:0; resize:none;}
.flowsheet_form .ext_css{ background:#fff;}
.flowsheet_form .red_prompt{ width:632px; height:109px; border:1px solid #f00;}

/* 信息发布流程 */
.flowsheet_full .title{ width:780px; height:80px; line-height:80px; font-size:25px; text-align:center; float:left; overflow:hidden; text-overflow:ellipsis; white-space:nowrap;}
.flowsheet_full .info{ width:780px; height:26px; line-height:26px; font-size:13px; text-align:center; color:gray; float:left;}
.flowsheet_full .summary{ width:750px; height:100px; float:left; margin:20px 0 20px 15px; background:#FBFCDC; border:1px solid #ccc; _display:inline;}
.flowsheet_full .summary span{width:100%; height:20px; line-height:20px; font-size:13px; font-weight:bold; color:#D14500; text-indent:5px; float:left;}
.flowsheet_full .summary p{ width:740px; height:70px; line-height:20px; color:#333; font-size:13px; text-indent:3em; float:left; padding:5px;}
.flowsheet_full .mains,.flowsheet_full .files_box{ width:750px; height:auto; float:left; margin-left:15px; overflow:hidden; _display:inline;}
.flowsheet_full .power_status{ width:760px; height:30px; float:left; padding-left:20px;}
.power_status div{ width:auto; height:auto; float:left; padding:5px 10px; margin-right:15px; border:1px solid #ccc;}
.power_status input{ float:left; margin-right:5px;}
.power_status label{ float:left; color:red;}

.only_space{ width:780px; height:20px; float:left;}

/* 按钮样式 adv_btn定位按钮父标签没有样式,销毁流程和信息发布共用 */
.adv_btn .buttons{ width:800px; height:26px; float:left; padding-top:1px; margin:0; background:url("../../public/flexigrid/css/images/wbg.gif") repeat-x 0 -3px;}
.adv_btn .buttons .button{ float:left; padding:1px; cursor:pointer}
.adv_btn .buttons .extbutton{ padding:0; border:1px solid #ccc;}
.adv_btn .buttons div{ float:left; padding-left:1px;}
.adv_btn .buttons .div{ padding:0; border-left:1px solid #fff; background:url("../../public/flexigrid/css/images/bg.gif") repeat-x scroll center top #FAFAFA;}
.adv_btn .buttons span{ float:left; padding:4px 5px 4px 22px;}
.adv_btn .buttons .afree{ width:70px; height:26px; float:left;}
.adv_btn .buttons .agree,.adv_btn .buttons .agin_submit{ background:url(../../public/flexigrid/css/images/accept.png) no-repeat 3px center;}	/* 提交 */
.adv_btn .buttons .back{ background:url(../../public/flexigrid/css/images/resetBtn.png) no-repeat 3px center;}	/* 退回 */
.adv_btn .buttons .approval,.adv_btn .buttons .modify{ background:url(../../public/ztree/css/zTreeStyle/img/diy/2.png) no-repeat 3px center;}	/* 转审批 */
.adv_btn .buttons .process{ background:url(../../public/ztree/css/zTreeStyle/img/diy/3.png) no-repeat 3px center;}	/* 流程图 */
/* 按钮样式结束 */

/* 展示附件 */
.flowsheet_full .files_box{ margin-top:20px;}
.es_files{ width:100%; height:24px; float:left; border-bottom:1px dotted #aaa;}
.es_files h2,.es_files p{ height:24px; line-height:24px; color:#555; float:left;}
.es_files h2{ width:600px;}
.es_files p{ width:150px;}
.es_files h2 a{ line-height:24px; color:#00f;}
.es_files h2 a:hover{ color:#555;}
.es_files p span{ line-height:24px; color:#00f;}

/* 图片展示 */
.task_image_list{ width:748px; height:auto; overflow:hidden; text-align:center; float:left; margin:20px 0 0 15px; padding:1px; border:1px solid #aaa; _display:inline;}
.task_image_list div{ width:748px; height:25px; float:left; margin-top:1px; border-top:1px dotted #aaa;}
.task_image_list img{ border:0;}
.task_image_list div h2{ width:648px; height:26px; line-height:26px; text-align:center; font-family:微软雅黑,airal,宋体; float:left; overflow:hidden; text-overflow:ellipsis; white-space:nowrap;}
.task_image_list div span{ width:100px; line-height:26px; text-align:right; font-family:微软雅黑,airal,宋体; float:left;}


/* 移入保留清册延长时间和延长说明框 */
.write_reason{ width:370px; height:auto; float:left;}
.write_reason .items{ width:350px; height:auto; float:left; padding:10px;}
.write_reason .items h2,.write_reason .items p{ width:100%; float:left;}
.write_reason .items h2{ line-height:24px; font-size:14px;}
.write_reason .items p input,.write_reason .items p textarea{ width:340px; height:18px; line-height:18px; font-size:14px; letter-spacing:1px; padding:3px; border:1px solid gray;}
.write_reason .items p textarea{ height:100px; resize:none;}
/* 移入保留清册延长时间和延长说明框结束 */

.flexigrid div.fbutton .redo{background:url(../img/redo.gif) no-repeat center left;}
.flexigrid div.fbutton .undo{background:url(../img/undo.gif) no-repeat center left;}
