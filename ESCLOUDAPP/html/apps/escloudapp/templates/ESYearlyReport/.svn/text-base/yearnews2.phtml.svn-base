<script type="text/javascript" >
$(document).ready(function(){
	// 编辑年报时填充数据
    var yearnews = <?php echo json_encode($yearnews);?>;
    $.each(yearnews, function(k, v){
		$("#field_y_"+k).val(v);
		$("#field_S_"+k).val(v);
    });
    // 标签页处理
    var $div_li =$("div.tab_menu ul li");
    $div_li.click(function(){
		$(this).addClass("selected")            
			   .siblings().removeClass("selected"); 
	    var index =  $div_li.index(this); 
		$("div.tab_box > div")   	
				.eq(index).show()  
				.siblings().hide();
	}).hover(function(){
		$(this).addClass("hover");
	},function(){
		$(this).removeClass("hover");
	});

    // 隐藏无权按钮
	if(!<?php echo $canEdit;?>){
		$("#yearSave").closest(".fbutton").hide();
		$("#yearRelation").closest(".fbutton").hide();
		$("#yearAutostat").closest(".fbutton").hide();
	}
	
	// 关闭按钮事件
	$("#yearClose").click(function(){
		if(typeof($.dialog.list["addYearDialog"])!='undefined'){
			$.dialog.list["addYearDialog"].close();
		}
		if(typeof($.dialog.list["yearDialog"])!='undefined'){
			$.dialog.list["yearDialog"].close();
		}
	});
	//验证档基2表的数字输入
	$("#tab_box").find("input[datatype='number']").keyup(function(){
		var epReg=/^(-?\d+)(\.\d+)?$/;
		if((!epReg.test(this.value))&&(this.value!='')){
			$(this).addClass("invalid-text").attr("title","请输入合法的数字");
			return false;
		}
		$(this).removeClass("invalid-text").attr("title","");
	});
	// 保存按钮事件
	var checkNum=0;
	$("#yearSave").click(function(){
		var saveYearnews = {};
		$("#tab_box").find("input").each(function(i, v){
			var datatype = $(this).attr("datatype");
			if(datatype=="number"){
				var eipReg=/^(-?\d+)(\.\d+)?$/;
				if((!eipReg.test($(this).val()))&&($(this).val()!='')){
					checkNum=1;
					$(this).addClass("invalid-text").attr("title","请输入合法的数字");
				}
				saveYearnews[$(this).attr("id").substring(8).toLowerCase()] = Number($(this).val());
			} else {
				saveYearnews[$(this).attr("id").substring(8).toLowerCase()] = $(this).val();
			}
		});
		if(checkNum==1){
			checkNum=0;
			$.dialog.notice({content:'输入的数字不合法，请重新输入！',icon:'warning',time:3});
			return false;
		}else if(checkNum==0){
			$.post($.appClient.generateUrl({ESYearlyReport:"save"},"x"), {yearnews:saveYearnews},function(data){
				if(data && data.result){
					$.dialog({id:'addYearDialog'}).close();
					$.dialog.notice({content:"年报保存成功",icon:"success", ok:true, time:2});
				} else {
					$.dialog.notice({content:"年报保存失败:" + data.msg,icon:"error",time:3});
				}
			}, "json");
		}
	});
	// 设置对应按钮事件
	$("#yearRelation").click(function(){
		var yearId = $("#field_y_id").val();
		var typeId = $("#field_y_typeid").val();
		$.ajax({
		    url:$.appClient.generateUrl({ESYearlyReport:'setRelation', yearId:yearId, typeId:typeId},'x'),
		    success:function(data){
		    	$.dialog({
		    		id:"yearConditionDialog",
			    	title:'设置对应',
			    	width:905,
			    	height:500,
		    	    padding: '0px 0px',
			    	content:data,
			    	fixed:true
			    });},
			    cache:false
		});
	});
	// 自动统计按钮事件
	$("#yearAutostat").click(function(){
		var yearId = $("#field_y_id").val();
		var typeId = $("#field_y_typeid").val();
		var managerLevel = $("#managerLevel").val();
		var provinceCode = $("#provinceCode").val();
		var orgId = $("#orgId").val();
		$.getJSON($.appClient.generateUrl({ESYearlyReport:'autoStat'},'x'),{yearId:yearId, typeId:typeId, managerLevel:managerLevel, provinceCode:provinceCode, orgId:orgId},function(yearstat){
			$.each(yearstat, function(k, v){
				$("#field_y_"+k).val(v);
		    });
			$.dialog.notice({content:"统计完毕，请查看数据是否准确。",icon:"success", time:2});
		});
	});
	//提交审批按钮事件
	$("#yearReport").click(function(){
		var datacsd=<?php echo json_encode($datacsdis);?>;
		if(datacsd=='自建年报'){
			var saveYearnews = {};
			$("#tab_box").find("input").each(function(i, v){
				var datatype = $(this).attr("datatype");
				if(datatype=="number"){
					var eipReg=/^(-?\d+)(\.\d+)?$/;
					if((!eipReg.test($(this).val()))&&($(this).val()!='')){
						checkNum=1;
						$(this).addClass("invalid-text").attr("title","请输入合法的数字");
					}
					saveYearnews[$(this).attr("id").substring(8).toLowerCase()] = Number($(this).val());
				} else {
					saveYearnews[$(this).attr("id").substring(8).toLowerCase()] = $(this).val();
				}
			});
			if(checkNum==1){
				checkNum=0;
				$.dialog.notice({content:'输入的数字不合法，请重新输入！',icon:'warning',time:3});
				return false;
			}else if(checkNum==0){
				$.post($.appClient.generateUrl({ESYearlyReport:"save"},"x"), {yearnews:saveYearnews},function(data){
					if(!data || !data.result){
						$.dialog.notice({content:"年报保存失败:" + data.msg,icon:"error",time:5});
						return false;
					}
				}, "json");
			}
		}
		submityear();
	});
});
</script>
