@CHARSET "UTF-8";
body,html{ margin:0; overflow:hidden;}

.init-style{ float:left; overflow:hidden;}
#ztree{background-color:#133959;border-radius:5px;}
h2,p{font-size: 12px;}

/* guolanrui 20141008 修改 查看详细信息按钮样式  bug:1116*/
.details{ width:16px; height:16px; background:url(../../public/flexigrid/css/images/edit.png) no-repeat; border:0; cursor:pointer;}

/* 详细信息 */
.log-detail{ width:600px; height:auto; float:left;}
.log-detail .log-form{ width:580px; height:auto; float:left; margin:8px; border:2px solid #ccc; overflow:hidden; _display:inline;}
.log-detail .log-form h2,.log-detail .log-form p{ float:left; line-height:30px; border-right:1px solid #ccc;margin: 0px;padding: 0px;}

.log-detail .log-form h2{ width:100px;  text-align:center; background:#eee;}
.log-detail .log-form h2.tall{ height:100px; line-height:100px;}

.log-detail .log-form p{ width:188px; text-indent:5px; overflow:hidden; text-overflow:ellipsis; white-space:nowrap;}

.log-detail .log-form p.long{ width:475px; border:0;}

.log-detail .log-form div{  width:475px; height:100px; float:left; border:0;}
.log-detail .log-form div textarea{ width:470px; height:100px; line-height:20px; font-size:13px; float:left; border:0; margin:0 0 0 5px; resize:none; _display:inline;}

.log-detail .log-form p.clear{ border:0;}
.log-detail .log-form i{ width:100%; height:1px; float:left; background:#ccc; overflow:hidden;}
.link{ width:16px; height:16px; border:0; background:url(../img/link.png) no-repeat right center; cursor:pointer;}
.ext-link{ width:16px; height:16px; border:0; background:none; cursor:default;}
.ext-details{ margin-right:4px;}

.format-xml{ width:700px; height:400px; float:left; font-size:14px; line-height:22px; overflow:auto; border:1px solid #ccc;}

.format-list-box{ width:700px; height:400px; float:left; overflow:auto; padding-top:5px;}
.format-list{ width:670px; height:auto; overflow:hidden; float:left; border:1px solid #aaa; background:#f5f5f5; margin:0 5px 5px;}
.format-list li{ width:100px; height:20px; line-height:20px; text-indent:5px; float:left; border-bottom:1px solid #fff;}
.format-list li.long{ width:565px; float:left; padding-left:5px;}
.format-list li.odd{background:#eee;}
.format-list li.event{background:#fff;}

.statisticsClass{width: 800px;height: 500px;}
.statisticsClass .statisticsTitle{width: 800px;height: 30px; margin: 10px 0 5px 10px;}
.statisticsClass .statisticsTitle .title{float: left;height: 20px;margin-right:5px; margin-top: 5px;}
.statisticsClass .statisticsTitle .selectSta{float: left;height: 20px;margin-right:10px;width: 150px;margin-top: 5px;}
.statisticsClass .statisticsTitle .button{float: left;height: 28px;color: #fff;line-height: 28px;padding: 0 5px 0px 5px;padding-left: 28px;_border-top-color: tomato;
_border-left-color: tomato;_filter: chroma(color =tomato);background-position-x: 5px;_border-top-color: tomato;cursor: pointer;background-color: #6f6e81; margin-right: 10px;}
.statisticsClass .statisticsTitle .button.setCondition{background: url(../img/folder_image.png) no-repeat
		5px;background-color: #6f6e81;}
.statisticsClass .statisticsTitle .button.clickStatistics{background: url(../img/documentManag.png) no-repeat
		5px;background-color: #6f6e81;}
.statisticsClass .statisticsTitle .button:HOVER {
	background-color: #133959;
}