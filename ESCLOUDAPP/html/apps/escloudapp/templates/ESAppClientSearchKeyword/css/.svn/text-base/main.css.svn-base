@charset "utf-8";
/* CSS Document */

html,body{ margin:0; padding:0; overflow:hidden;}

.leftDiv{ width:220px; float:left; overflow:hidden;margin-right:10px;background-color:#133959;border-radius:5px;}
.rightDiv{ float:left; overflow:hidden;}

.leftDiv div{ width:100%; height:auto; overflow:hidden; float:left;}
.leftDiv a, .leftDiv h2{ width:220px; height:30px; font:normal normal normal 14px/30px 微软雅黑,黑体,宋体,arial; text-indent:20px; float:left;}
.leftDiv h2{ color:#3E78FD; font-weight:bold;}
.leftDiv a{ font-size:12px; text-decoration:none; color:#fff;text-indent:80px;}
.leftDiv a:hover{ background:#f5f5f5;width:220px;color:#133959;}
.leftDiv a.selected{ background:#eee;color:#133959;}
.leftDiv span{ color:#000;}
.opens{ width:16px; height:16px; display:inline-block; background:url(../img/editor.gif) no-repeat no-repeat; cursor:pointer;}

.ESAppClientSearchKeywordContainer{
	width:96%;
	margin-left:2%;
}
