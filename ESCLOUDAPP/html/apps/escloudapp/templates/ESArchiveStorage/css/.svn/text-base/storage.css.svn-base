.main .left {
	width: 17%;
	background: #fff;
	height: 620px;
	float: left;
	border: 1px solid #ccc;
	overflow-x: auto;
}

.main .right {
	width: 79%;
	background: #fff;
	height: 620px;
	float: right;
	overflow: auto;
}

.main .right .item {
	width: 100%;
	height: 50px;
	border-bottom: 1px solid #ccc;
}
.right .item font{
	font-size:12px;
	color:#bbb;
}