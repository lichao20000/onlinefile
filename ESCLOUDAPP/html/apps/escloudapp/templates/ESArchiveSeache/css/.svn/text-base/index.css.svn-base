.product-list .pl-hr{ width:70%; height:40px; float:left; background:#f5f5f5;}
.product-list .pl-hr a{ width:53px; height:40x; line-height:40px; font-size:16px; font-family:微软雅黑,arial; _font-family:微软雅黑,arial !important; text-decoration:none; color:#333; float:right; margin-right:30px; padding-left:10px;cursor:pointer; background:url(../images/clear.png) no-repeat 0 9px;}
