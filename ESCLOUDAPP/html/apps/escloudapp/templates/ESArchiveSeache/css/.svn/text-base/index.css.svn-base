/* 购物车样式dotted */
.product-list{ width:350px; height:auto; overflow:hidden; position:absolute; top:26px; right:-1px; border:1px solid #aaa; border-top:0; background:#fff url(../images/product-list.gif) no-repeat -240px 0; display:none;}
