.product-list .pl-sb{ width:30%; height:40px; float:right; background:#f5f5f5;}
.product-list .pl-sb a{ width:73px; height:40x; line-height:40px; font-size:16px; font-family:微软雅黑,arial; _font-family:微软雅黑,arial !important; text-decoration:none; color:#333; float:right; margin-right:10px; padding-left:30px; background:url(../images/go.gif) no-repeat 0 9px;}
