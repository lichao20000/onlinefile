
.formStartPage {
	width:760px;
	height:460px;
	overflow: auto;
}

.formStartPage .topTBar {
	padding-top:4px;
	padding-bottom:4px;
	background-color:#FFF;
	position: relative;
	border: 1px solid #ccc;
	border-top: 0px;
	overflow: hidden;
}
.formStartPage .topTBar div.tbarDiv {
	float: left;
	clear: both;
	padding: 1px;
}
.formStartPage .topTBar div.tbarDiv div.tbarButton {
	float: left;
	display: block;
	cursor: pointer;
	padding: 1px;
	background-color: rgb(74, 139, 194);
	margin-left:5px;
	color:#FFF;
	border: 0px none;
    outline: 0px none;
}
.formStartPage .topTBar div.tbarDiv div.tbarButton:hover,.formStartPage .topTBar div.tbarDiv div.tbarButton.fbOver {
	border: 0px none;
    outline: 0px none;
	background-color: #41b3e5;
}
.formStartPage .topTBar div.tbarDiv div.tbarButton div {
	float: left;
	padding: 1px 3px;
}
.formStartPage .topTBar div.tbarDiv div.tbarButton span {
	float: left;
	display: block;
	padding: 3px;
}
.formStartPage .topTBar div.tbarDiv div.tbarButton .add {
	background: url(img/tbar/add.png) no-repeat center left;
}
.formStartPage .topTBar div.tbarDiv div.tbarButton .edit {
	background: url(img/tbar/edit.jpg) no-repeat center left;
}
.formStartPage .topTBar div.tbarDiv div.tbarButton .delete {
	background: url(img/tbar/delete.png) no-repeat center left;
}

.formStartPage .formBuilderDiv {
	width: 100%;
	display: block;/** Ĭ����ʾ **/
}

.formStartPage .formBuilderDiv .formBuilderPanelDiv {
	width: 744px;
	MARGIN-RIGHT: auto;
	MARGIN-LEFT: auto; 
}

.formStartPage .formBuilderFiles,.formBuilderDatas,.formBuilderMsgs {
	width:100%;
	MARGIN-RIGHT: auto;
	MARGIN-LEFT: auto; 
	display: none;/** Ĭ������ **/
}

.formStartPage .formBuilderFiles .filesDiv {
	width:100%;
	height: 374px;
	overflow:auto;
	float:left;
}

.formStartPage .formBuilderDatas .formStart_grid {
	width:100%;
	height: 374px;
	overflow:auto;
	float:left;
}

.formStartPage .formBuilderDiv .floatLeft {
	float: left;
	padding: 0 0 0 25px;
}
.formStartPage .formBuilderDiv .allowFloat {
	clear: none !important;
}
.formStartPage .formBuilderDiv .stopFloat {
	clear: both !important;
}

/** tab��ǩչ����Ҫcss **/
.formStartPage .tabpanel{ width:100%; height:35px; float:left; padding-top:10px; overflow:hidden;}
.formStartPage .tabpanel .subnav{width:100%; height:30px; float:left; padding-left:10px; border-bottom: 5px solid #133959;}
.formStartPage .tabpanel .subnav li{ line-height:30px; color:#fff; float:left; padding:0 10px; margin-right:5px; background: #6f6e81; cursor:pointer; border-radius:5px 5px 0 0;}
.formStartPage .tabpanel .subnav .defalutTagOpen{ background: #133959;}
.formStartPage .tabpanel .subnav .not-allowed{color:#ffffff; cursor:not-allowed;}
.formStartPage .Tags{ float:left; height:auto; overflow:hidden;}

.formStartPage .buttoncss { width:100%; height:35px; float:left;padding-left:5px;padding-top:5px; border-bottom:1px solid #ccc;}
.formStartPage .buttoncss div {
	float: left;
	display: block;
	cursor: pointer;
	float: left;
	padding: 1px 3px;
	background-color: #6f6e81;
	margin-left:5px;
	color:#FFF;
	border: 0px none;
    outline: 0px none;
}
.formStartPage .buttoncss div:hover,.formStartPage .buttoncss div.fbOver {
	border: 0px none;
    outline: 0px none;
	background-color: #133959;
}
.formStartPage .buttoncss div span {
	float: left;
	display: block;
	padding: 3px;
}

.formStartPage .buttoncss .add {
	background: url(../img/add.png) no-repeat center left;
}
.formStartPage .buttoncss .edit {
	background: url(../img/edit.png) no-repeat center left;
}
.formStartPage .buttoncss .delete {
	background: url(../img/del.png) no-repeat center left;
}

.formStartHanderForm{width:400px;overflow-y:hidden;overflow-x:hidden}
.formStartHanderForm ul{width: 380px;text-align:center;}
.formStartHanderForm ul li{width:380px;margin-left:10px;float:left;margin-top:10px;color:#000000;}
.formStartHanderForm span{width:80px;height:30px;line-height:30px;text-align:left;float:left;}
.formStartHanderForm ul li input{float:left;line-height:20px;}
.formStartHanderForm ul li label{float:left;margin-top: 5px;}
.formStartHanderForm ul li select{width:165px;height:25px;float:left;line-height:25px;}
.formStartHanderForm .esbottom{width:100%;float:left;margin-top:20px;}

.formStartHanderForm .selectOwner{ width:250px; height:20px; border:1px solid #aaa; background:#fff; float: left; margin-top: 2px; line-height: 20px;position: absolute;left: 100px;}
.formStartHanderForm .selectOwner input{ width:227px; height:20px; line-height:20px; float:left; border:0; _line-height:23px; color: gray;}
.formStartHanderForm .selectOwner span{ width:20px; height:20px; float:left;  background:url(../img/search.gif) no-repeat center center; cursor:pointer;}

.formStartHanderForm a{ cursor: pointer;}

