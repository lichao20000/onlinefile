class ProxyEscloudappEscloud_appConfigws extends AgentProxyAbstract
{
	const SERVICE_NAME = "appConfigServer";
	 /**
	 * 获取总记录数
	 * 
	 */
	public function getAppConfigCount($wb)
	{
		$url = "getAppConfigCount";
		return $this->post(self::SERVICE_NAME,$url,$wb,"text/plain;charset=UTF-8");
	}
	/**
	 * 获取所有系统配置
	 */
	public function getAllAppConfigList($canshu)
	{
		$url = "getAppConfigList";
		return $this->post(self::SERVICE_NAME, $url,$canshu,"application/json;charset=UTF-8");
	}
	public function editApp($data){
		$url = "updateAppConfig";
		return $this->post(self::SERVICE_NAME, $url,$data,"application/json;charset=UTF-8");
	}

}