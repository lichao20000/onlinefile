.fieldset .input_add_background input.register_input_ot{/* background-color:#f9f9f9;border:0 none; */color:#4F4F4F;font-size:12px;height:18px;outline:medium none;width: 145px;padding:1px;margin:5px;}
.fieldset .input_add_long_background{/* background:url("../img/input_long.jpg") no-repeat scroll 100% 100% transparent; */background-position: center;height:30px;width:286px;float:left;margin:0 0 15px 0;display:inline;}
