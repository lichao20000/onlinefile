.error input,.error textarea{border:1px solid #cb5234;background:#fff6f6}
.errorrr{border:1px solid #cb5234;background:#fff6f6  !important}
.error input:focus,.error textarea:focus{border-color:#cb5234;-webkit-box-shadow:0 0 7px rgba(255,0,0,0.15);-moz-box-shadow:0 0 7px rgba(255,0,0,0.15);box-shadow:0 0 7px rgba(255,0,0,0.15)}
select{background:white;padding-right:3rem}
select.small{padding-right:2.5rem}
label{font-weight:bold;margin:0 0 .25rem;display:block;font-size:1rem}
label.radio,label.checkbox{padding-left:1.5rem;cursor:pointer}
label.radio input[type="radio"],label.checkbox input[type="radio"],label.radio input[type="checkbox"],label.checkbox input[type="checkbox"]{margin:-2px .5rem 0 -1.5rem;vertical-align:middle}
label.checkbox input[type="checkbox"]{margin-top:-1px}
label.disabled{color:#9e9ea6}
label select{margin-top:.25rem}
label.select{position:relative}
label.select:after{content:"\f0d7";font-family:'FontAwesome';position:absolute;right:22px;bottom:20px;font-size:24px;pointer-events:none}
label.select.disabled:after{color:#9e9ea6}
label.select.small:after{right:1rem;bottom:14px;font-size:20px}
@-moz-document url-prefix(){label.select:after{content:""}
}

.headerheader {
background: #fff;
height: 5rem;
-webkit-box-shadow: 0 1px 4px rgba(0,0,0,0.15);
-moz-box-shadow: 0 1px 4px rgba(0,0,0,0.15);
box-shadow: 0 1px 4px rgba(0,0,0,0.15);
position: fixed;
top: 0;
left: 0;
right: 0;
z-index: 100;
}

.alerterror.alert_error {
border-left-color: #cb5234;
}

.alerterror {
padding: .75rem 1rem .75rem 3rem;
border: 1px solid #e8e8e8;
background: white;
color: #555459;
border-left-width: 5px;
margin: 0 auto 1rem;
-webkit-border-radius: .25rem;
-moz-border-radius: .25rem;
border-radius: .25rem;
}

.fa-warning:before, .fa-exclamation-triangle:before {
content: "\f071";
}