			$("#submitBtn", _parentWin.document).removeClass("dd-no-submit") ;
		}
		window.close();
	});
	$("#parentDiv_input_btn_disagree").click(function(){
		var _parentWin =  window.opener ;  
		_parentWin.registerinfo.checked = false ;
