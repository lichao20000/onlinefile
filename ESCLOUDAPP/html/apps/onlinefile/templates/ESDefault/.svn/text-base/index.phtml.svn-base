<script type="text/javascript">
 initSWFUpload();
function initSWFUpload(){
	var files = [];
	$uploadObj = new SWFUpload({
		//提交路径
// 		upload_url: "http://168.168.169.231:8080/fileStoreServer/rest/fileServer/uploadFilesOfOnlineFile/1/46e56d00167c1033a620b8763f4aa704",
		is_onlinefile_upload:"1",
		file_post_name: "file.txt",		
		file_size_limit : "1048576",	// 100MB  longjunhao 20140822 修改为1024MB=1GB
		file_types : "*.*",
		file_types_description : "所有文件",
		file_upload_limit : "15",  //wangwenshuo 20160120 修改为15
		file_queue_limit : "15",	  //wangwenshuo 20160120 修改为15
		use_query_string : true,

		// 事件处理
		swfupload_loaded_handler : swfuploadLoaded,
		file_dialog_start_handler : fileDialogStart,
		file_queued_handler : fileQueued,
		file_queue_error_handler : fileQueueError,
		file_dialog_complete_handler : fileDialogComplete,
		upload_start_handler : uploadStartOfprop,
		upload_progress_handler : uploadProgress,
		upload_error_handler : uploadError,
		upload_success_handler : uploadSuccess,
		upload_complete_handler : uploadComplete,

		// 按钮的处理
		button_image_url : "/apps/onlinefile/templates/public/SWFUpload/img/uploadbtn.png",
		//button_text:"上传文件",
		button_placeholder_id : "btnAdd",
		button_window_mode : SWFUpload.WINDOW_MODE.TRANSPARENT,
		button_width: 78,
		button_height: 35,
		button_cursor: SWFUpload.CURSOR.HAND,
		
		// Flash文件地址设置
		flash_url : "/apps/onlinefile/templates/public/SWFUpload/js/swfupload.swf",
		
		custom_settings : {
			progressTarget : "fsUploadProgress",
			cancelButtonId : "btnCancel",
			startButtonId : "btnStart",
			// 上传成功的回调函数
			uploadSuccess : function(file, data, remainder){
				var companyId = window.companyid;
				/** 如果是我的文档创建文件夹 那么companyId应该为‘user_’+userId */
				var isMyDocument = $("#myDocument").hasClass("active");
				if(isMyDocument){
					companyId = "user_"+window.userId;
				}
				// 插入数据
				var json = eval('('+data+')');
				$.post($.appClient.generateUrl({ESDocumentCenter:'insertNewFileAfterUpload'},'x'),
						{classId:json.classId,fileName:json.filename,size:json.fileSize,md5:json.contentMD5,type:json.type,
					openLevel:json.openLevel,fileId:json.fileId,userId:window.userId,companyId:companyId},
					function(rst){
						var currentFolderId = $('#file-breadcrumbs li.last').attr('data-folder-id');
						documentCenter.getFileList(currentFolderId);
						// 在群组聊天里发送上传文件的消息
						if (!isMyDocument) {
							$.WebIM.sendFileMsg(rst, "upload");
						}
				});
			}
		},
		
		// Debug 设置
		debug: false
	});
	$("#btnCancel").click(function(){
		cancelQueue($uploadObj);
// 		fsUploadProgressHide();
		});
	$("#btnStart").click(function(){
		$("#fsUploadProgress").css('display','block');
		// 获取本次上传的upload_url
		//jQuery.getJSON(window.siteUrl+'/fileStoreMainServer/rest/mainFileServer/dispatchFileUploadUrl?callback=?',
		var url = window.siteUrl+'/fileStoreMainServer/rest/mainFileServer/dispatchFileUploadUrl?callback=?';
		var data = {};
		var ret = addSecurityPart(url, data, window.token, window.u, window.jsessionid);
		jQuery.getJSON(url, ret.data,
			function(json) {
			var url = json.url;
			if (url != "") {
				$uploadObj.setUploadURL(url);
				startQueue($uploadObj);
			} else {
				dialog({
					title: '遇到问题',
					content: '文件服务器找不到了，我们正在努力解决...',
					ok: true,
					okValue:'确定'
				}).showModal();
			}
		});
// 		startQueue($uploadObj);
	});
}; 
</script>