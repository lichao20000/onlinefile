       {{if $value.openLevel == "3"}}
		<span class="glyphicon glyphicon-lock" style="float: left;<!--{{if $value.hasRight=="1"}} color: rgb(132, 202, 135){{/if}}-->" tilte="私密文件"></span>
       {{/if}}
     <span class="filetype_icon folder"></span>
    </a>
	<div class="file-name" serialNumber="{{$value.serialNumber}}" fileName="{{$value.fileName}}"  title="{{$value.fileName}}">{{$value.serialNumber}}-{{$value.fileName}}</div>
    {{else}}
	<a class="file_preview_link icon" serialNumber="{{$value.serialNumber}}" fileName="{{$value.fileName}}"  title="{{$value.fileName}}.{{$value.type}}">
	{{if myDocument}}
	
	{{else}}
		 {{if $value.openLevel == "3"}}
			<span class="islockcolor glyphicon glyphicon-lock" style="float: left;;<!--{{if $value.hasRight=="1"}} color: rgb(132, 202, 135);{{/if}}-->" tilte="私密文件"></span>
	    {{/if}}
	    {{if $value.openLevel == "1"}}
	   		<span class="isearthcolor glyphicon glyphicon-globe" style="float: left;<!--{{if $value.hasRight=="1"}} color: rgb(132, 202, 135){{/if}}-->" tilte="公司文件"></span>
	    {{/if}}
	{{/if}}
    	<span class="filetype_icon s48 {{$value.type}}"></span>
    </a>
	{{if $value.fileName.length > 23}}
    	<div class="file-name" serialNumber="{{$value.serialNumber}}" fileName="{{$value.fileName}}"  title="{{$value.fileName}}.{{$value.type}}">{{$value.serialNumber}}-{{$value.fileName.substr(0,23)}}...</div>
    {{else}}
		<div class="file-name" serialNumber="{{$value.serialNumber}}" fileName="{{$value.fileName}}"  title="{{$value.fileName}}.{{$value.type}}">{{$value.serialNumber}}-{{$value.fileName}}.{{$value.type}}</div>
