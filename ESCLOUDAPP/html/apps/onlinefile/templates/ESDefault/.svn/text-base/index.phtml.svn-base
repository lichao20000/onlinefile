<script id="onlineUsersCommnutyList" type="text/html">
{{each data as row}}
	<div class="wt-row-thread-item" onmouseover="nTab1(this)" onmouseout="out(this)">
		<div class="thread-item-inner">
			<div class="row">
				<div class="col-sm-1 col-xs-2 avatar avatar-40" >
					<span class="avatar-face" >
						{{if row.imgurl != ''}}
							<img id="indexheadimage" src="{{row.imgurl}}" style ="border-radius: 50%;width: 40px;height: 40px;background: #f7f7f7;box-shadow: 0 0 1px #ddd; margin-left: 14px;">
						{{/if}}
					</span>
				</div>
				<div class="col-sm-11 col-xs-10 text">
					<div class="row primary">
						<div class="col-sm-9 col-xs-9 thread-title">
							<h4 style="margin-left: -3%;">
								<a href="javascript:void(0)" title="{{row.title}}" onclick="onlineCommnunityClick({{row.userid}},'{{row.pubtype}}',0)" style="padding-left: 6%;">
									{{if row.pubtype == 'newuser'}}
										<span class="label label-success">新手上路</span>
									{{else if row.pubtype == 'userreply'}}
										<span class="label label-warning">用户反馈</span>
									{{else if row.pubtype == 'usertec'}}
										<span class="label label-info">用户技巧</span>
									{{else if row.pubtype == 'guanfang'}}
										<span class="label label-danger">产品公告</span>
									{{/if}}
									
									{{if row.title.length>20}}
										{{row.title.substring(0,20)+'...'}}
									{{else}}
										{{row.title}}
										{{showTitle}}
									{{/if}}	
								</a>
							</h4>
						</div>
						<div class="col-sm-3 thread-counts">
							{{if username == row.user_name || user_name == 'admin'}}
							  <span class="hover-show">
							  	<a href="javascript:;" class = "glyphicon glyphicon-trash" style="float:right;margin-right:20px;dispaly:none" onclick="deleteCommunityArtical({{row.userid}},'{{row.pubtype}}')"></a>
							  </span>
							{{/if}}
							<span class="thread-total-comments">{{row.total}}</span>
						</div>
					</div>
					<div class="row secondary hidden-xs">
						<div class="col-sm-7-col-xs-7-thread-meta">
							<span class="ng-binding">{{row.username}}</span>
							发布于 
							<span class="ng-binding">{{row.date}}</span>
						</div>

						<div class="col-sm-4-thread-comment-meta-ng-scope">
							{{if row.maxpldate != '' && row.maxpldate != null}}
							<span class="ng-binding">{{row.fullName}}</span>
							最后回复于
							<span class="ng-binding">{{row.maxpldate}}</span>
							{{/if}}	
						</div>
					</div>
				</div>
			</div>
		</div>		
	</div>
{{/each}}
{{if data == ''}}
<div class="empty-panel">
		<i class="glyphicon glyphicon-file"></i>
		<br>
	<span>没有贴子</span>
</div>
{{/if}}
</script>

