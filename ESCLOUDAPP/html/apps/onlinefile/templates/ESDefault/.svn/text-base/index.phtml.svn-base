<script id="file_upload_version_template" type="text/html">
<div id="file_upload_version_confict" style="position: relative;">
	{{each files}}
		{{if $value.extentions =='false' }}
			<div class="alert alert-info {{if $value.owner!=<?php echo $userId;?>}}alert alert-danger{{/if}}" style="margin-bottom:3px;padding: 2px;">
				<span class="alert-danger glyphicon glyphicon-remove-sign" aria-hidden="false" title="文件无后缀名"></span>
				{{$value.fileName}}
				<span style="float: right;font-family: &quot;Microsoft Yahei&quot;,微软雅黑,&quot;Helvetica Neue&quot;,Helvetica,Arial,sans-serif,宋体;font-size: 14px;">无后缀名不能上传</span>
			</div>
		{{else if $value.openLevel =='3' }}
			<div class="alert alert-info {{if $value.owner!=<?php echo $userId;?>}}alert alert-danger{{/if}}" style="margin-bottom:3px;padding: 2px;">
				{{if $value.owner!=<?php echo $userId;?>}}
					 <span class="alert-danger glyphicon glyphicon-remove-sign" aria-hidden="false" title="存在私密文件"></span>
			 	{{else}}
					 <span class="alert-info glyphicon glyphicon-exclamation-sign" aria-hidden="true" title="存在同名文件"></span>
				{{/if}}
				{{$value.fileName}}
					{{if $value.owner!=<?php echo $userId;?>}}
						<span style="float: right;font-family: &quot;Microsoft Yahei&quot;,微软雅黑,&quot;Helvetica Neue&quot;,Helvetica,Arial,sans-serif,宋体;font-size: 14px;">存在他人私密文件,请重命名后上传</span>
					{{/if}}
			</div>
		{{else}}
			<div class="alert alert-success {{if $value.isExist=='true'}}alert alert-info{{/if}}" style="margin-bottom:3px;padding: 2px;">
				{{if $value.isExist=='true'}}
					 <span class="alert-info glyphicon glyphicon-exclamation-sign" aria-hidden="true" title="存在同名文件"></span>
			 	{{else}}
					 <span class="alert-success glyphicon glyphicon-ok-circle" aria-hidden="true" title="可直接上传"></span>
				{{/if}}
				{{$value.fileName}}
			</div>
		{{/if}}
	{{/each}}
<div>
</script>


<script type="text/javascript">
var browser=navigator.appName 
var b_version=navigator.appVersion 
var version=b_version.split(";"); 
/**    此处判断一下，当version数组长度小于1时候不走下面的判断，不然低版本谷歌浏览器会报错（包含XP）   **/
if(version != undefined && version.length >1 ){
	var trim_Version=version[1].replace(/[ ]/g,""); 
	if(browser=="Microsoft Internet Explorer" && trim_Version=="MSIE6.0") 
	{ 
		 $("#mask").css("height",$(document).height());  
	     $("#mask").css("width",$(document).width());  
	     $("#mask").show();  
	     $("#browser_ie").show();  
	     $("#main-body").hide();
	} 
	else if(browser=="Microsoft Internet Explorer" && trim_Version=="MSIE7.0") 
	{ 
		 $("#mask").css("height",$(document).height());  
	     $("#mask").css("width",$(document).width());  
	     $("#mask").show();  
	     $("#browser_ie").show();  
	     $("#main-body").hide();
	} 
	else if(browser=="Microsoft Internet Explorer" && trim_Version=="MSIE8.0") 
	{ 
	//alert("IE 8.0"); 
		 $("#mask").css("height",$(document).height());  
	     $("#mask").css("width",$(document).width());  
	     $("#mask").show();  
	     $("#browser_ie").show();  
	     $("#main-body").hide();
	}
}

</script>

