		{{/if}}
	{{/if}}
  {{else}}
	<a class="file_tool_menu hidden" title="更多" target-id="{{$value.id}}" isfile="{{$value.isFile}}"><span class="glyphicon glyphicon-chevron-down"></span></a>
    {{if $value.isFile == 0}}
