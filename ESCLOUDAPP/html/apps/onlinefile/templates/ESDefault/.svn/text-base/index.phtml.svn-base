	{{else}}
		<div class="column column-creator" type="creator" ordertype="4" onclick="sortList(this);">上传人 <span class="{{if orderField=="4" && orderType=="asc"}}caret-desc{{else}}caret{{/if}}"></span></div>
		<div class="column column-date" type="uploadDate" ordertype="5" onclick="sortList(this);">上传日期 <span class="{{if orderField=="5" && orderType=="asc"}}caret-desc{{else}}caret{{/if}}"></span></div>
		<div class="column column-openlevel" type="openLevel" ordertype="6" onclick="sortList(this);">公开级别 <span class="{{if orderField=="6" && orderType=="asc"}}caret-desc{{else}}caret{{/if}}"></span></div>
	{{/if}}
  </div>
{{/if}}
{{each files}}
	{{if $value.isFile == 0}}
  		<div id="file_id_{{$value.id}}" ondrop="drop(event,this)"  ondragover="allowDrop(event,this)" class="file-item {{myDocument}} {{if viewType=="list"}}list-type{{else}}icon-type{{/if}} dragTargetA" isfile="{{$value.isFile}}" folderid="{{$value.classId}}" ownerName="{{$value.userName}}" fileid="{{$value.id}}" filename="{{$value.fileName}}" filetype="{{$value.type}}" idseq="{{$value.idSeq}}" filepath="{{$value.fileId}}" filetitle="{{$value.fileName}}.{{$value.type}}" creatorid="{{$value.userId}}"  owner="{{$value.owner}}" owner_v1="{{$value.owner_v1}}" isMember="{{isMember?"true":"false"}}" version="{{$value.version}}" size="{{$value.size}}" type="{{$value.type}}" openlevel="{{$value.openLevel}}" createTime="{{$value.createTime}}">
 	{{else}}
  		<div id="file_id_{{$value.id}}" draggable="true"  ondragend="dragend(event,this)" ondragstart="drag(event)" class="file-item {{myDocument}} {{if viewType=="list"}}list-type{{else}}icon-type{{/if}} dragTargetA" isfile="{{$value.isFile}}" folderid="{{$value.classId}}" ownerName="{{$value.userName}}" fileid="{{$value.id}}" filename="{{$value.fileName}}" filetype="{{$value.type}}" idseq="{{$value.idSeq}}" filepath="{{$value.fileId}}" filetitle="{{$value.fileName}}.{{$value.type}}" creatorid="{{$value.userId}}"  owner="{{$value.owner}}" owner_v1="{{$value.owner_v1}}" isMember="{{isMember?"true":"false"}}" version="{{$value.version}}" size="{{$value.size}}" type="{{$value.type}}" openlevel="{{$value.openLevel}}" createTime="{{$value.createTime}}" isDownload="{{$value.isDownload}}" isLook="{{$value.isLook}}" {{if !myDocument}} ownerv1username="{{$value.ownerV1UserName}}"{{/if}}>
	{{/if}}

  {{if viewType=="list"}}
	<div class="column column-name {{if myDocument}}s30{{/if}}">
	  {{if $value.isFile == 0}}
		<a class="file_preview_link icon" title="{{$value.fileName}}">
     		<span class="filetype_icon s36 folder"></span>
    	</a>
		<a class="file-name" serialNumber="{{$value.serialNumber}}" fileName="{{$value.fileName}}"   title="{{$value.fileName}}">{{$value.serialNumber}}-{{$value.fileName}}</a>
      {{else}}
		<a class="file_preview_link icon" title="{{$value.fileName}}.{{$value.type}}">
      		<span class="filetype_icon s24 {{$value.type}}"></span>
    	</a>
    	<a class="file-name" serialNumber="{{$value.serialNumber}}" fileName="{{$value.fileName}}"  title="{{$value.fileName}}.{{$value.type}}">{{$value.serialNumber}}-{{$value.fileName}}.{{$value.type}}</a>
      {{/if}}
	</div>
	<!--liuwei 增加类型显示-->
	<div class="column column-type">{{if $value.isFile == 1}}{{$value.type}}{{else}}{{'文件夹'}}{{/if}}</div>
	<div class="column column-size">{{if $value.isFile == 1}}{{unitDisplay $value.size}}{{/if}}</div>
	{{if myDocument}}
		<!--<div class="column column-originalPath" title="{{$value.folderPathName}}">{{$value.folderPathName}}</div>-->
		<div class="column column-date"><span>{{showData $value.createTime}}</span></div>
	{{else}}
		<div class="column column-creator">{{$value.fullName}}</div>
		<div class="column column-date"><span>{{showData $value.createTime}}</span></div>
		{{if $value.openLevel == 0}}
			<div class="column column-openlevel"></div>
