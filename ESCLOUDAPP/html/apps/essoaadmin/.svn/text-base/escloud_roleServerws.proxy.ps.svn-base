class ProxyEssoaadminEscloud_roleServerws extends AgentProxyAbstract
{
	const SERVICE_NAME = "roleServer";
	const CONSOLESERVICE_NAME= "consoleServer";
	 /**
	 * 获取总记录数
	 * 
	 */
	public function getCountAll($wb)
	{
		$url = "getCount";
		return $this->post(self::SERVICE_NAME,$url,$wb,"application/json;charset=UTF-8");
	}
 
	/**
	* 获取所有角色信息
	*/
	public function getAllRoleServer($canshu)
	{
		$url = "getDataList";
		return $this->post(self::SERVICE_NAME, $url,$canshu,"application/json;charset=UTF-8");
	}
	  
	 /**
	 *获取应用列表
	 */
	 public function getAppList($canshu){
	    $url = "getAppList";
		return $this->post(self::CONSOLESERVICE_NAME, $url,$canshu,"application/json;charset=UTF-8");
	 }
	 	 /**
	 *添加角色
	 */
	 public function addRole($canshu){
	    $url = "saveData";
		return $this->post(self::SERVICE_NAME, $url,$canshu,"application/json;charset=UTF-8");
	 }
	  /**
	 *删除角色列表
	 */
	 public function deleteRoleList($canshu){
	        $url = "delData";
		    return $this->post(self::SERVICE_NAME, $url,$canshu,"application/json;charset=UTF-8");
	 }
	 
}