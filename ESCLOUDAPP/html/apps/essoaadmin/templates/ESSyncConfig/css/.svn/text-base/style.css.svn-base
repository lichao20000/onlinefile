@CHARSET "UTF-8";
body,html{margin:0px;overflow:hidden;}
input { margin:0; padding:0;}
ol,ul{ margin:0; padding:0; list-style-type:none;}
h2,p,form,table{ margin:0; padding:0; font-weight:normal;}

.configItem{width:350px;overflow-y:auto;overflow-x:hidden;height:180px;}
.configItem ul{width: 340px;text-align:center;}
.configItem ul li{width:310px;margin-left:20px;float:left;margin-top:10px;}
.configItem span{width:100px;height:30px;line-height:30px;text-align:left;float:left;}
.configItem ul li input{height:20px;float:left;line-height:20px;width:200px;}
.configItem ul li select{width:205px;height:25px;float:left;line-height:25px;}
