@charset "utf-8";
/* @author liuhezeng  CSS Document */

/** 左侧功能区 **/
.mainFunContent {
	float:left;
	margin-right:20px;
	height:500px;
	
}

.centerFunc{
	height:500px;
	align:center;
	margin:0 auto;
	width:1140px;
	padding-left:100px;
}

.mainFunContent{
	
	width:860px;
}

/*   换行符  */
.clearFloat {
	clear:both;
}

.lineModelLeft1,.lineModelRight1,.lineModelLeft2,.lineModelRight2,.lineModelLeft3,.lineModelRight3{
	cursor:pointer;
	width:340px;
	height:170px;
}
.lineModelRight1Login
{
	cursor:pointer;
	width:170px;
	height:170px;
}

.lineModelRight1,.lineModelLeft2,.lineModelRight2,.lineModelLeft3,.lineModelRight3{	
	overflow-y: auto;
}



.lineModelLeft1 {
	float:left;
	background-color: #133959;
}
.lineModelRight1 {
	background-color: #133959;
	float:left;
	margin-left:10px;
}
.lineModelRight1Login{
	background-color:#e8687f;
	margin-left:10px;
	float:left;
 	margin-left:10px;
}

.loginModelText{
	margin-top:65px;
	margin-left:55px;
	text-algin:center;
	font-size:28px;	
	color:#ffffff;
	font-weight:bolder;
}

.lineModelLeft2 {
	float:left;
	background-color: #133959;
	margin:10px 10px 0px 0px;
}
.lineModelRight2 {
	background-color: #133959;
	float:left;
	margin-top:10px;
}
.lineModelLeft3 {
	float:left;
	background-color: #133959;
	margin:0px 0px 0px 10px;
}
.lineModelRight3 {
	background-color: #133959;
	float:left;
	margin-top:10px;
	margin-left:10px;
}

.lineModelRight14{
	cursor:pointer;
	background-color:#db562d;
	float:left;
 	margin-left:10px;
 	height:170px;
	width:320px;
}

.lineModelRight33{
	cursor:pointer;
	float:left;
	margin-left:20px;
	background-color:#af1a40;
	height:350px;
	width:500px;
	margin-top:-170px;
}

/**  登录区 **/

.loginSectionContainer {
	position:absolute;
	width:400px;
	height:400px;
}


.dateSection{
	margin-top:50px;
}
